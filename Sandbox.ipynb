   "source": [
    "import re\n",
    "\n",
    "with open('./data/txt.txt') as f:\n",
    "    txt = f.read()\n",
    "\n",
    "part = '(\\[언어\\]|\\[사회\\]|\\[예술\\]|\\[과학\\]|\\[기술\\]|\\[인문\\])' \n",
    "groups = re.compile(r'(%(s)s)((.|\\n)+?)(%(s)s)'%{\"s\": part}).findall(txt)\n",
    "docs = [g[2] for g in groups]"
   ]
  },
